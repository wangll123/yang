int jsq1 ()
{

    int a,b,c ;
    char fh;

    printf("è¯·è¾“å…¥å·¦å€¼\n");
    scanf("%d",&a);
    printf("è¯·è¾“å…¥å³å€¼\n");
    scanf("%d",&b);

    char lj[1024];//æ¸…ç©ºç¼“å­˜å™¨ç©ºé—´
    fgets(lj,1024,stdin);

    printf("è¯·è¾“å…¥è¿ç®—ç¬¦\n");
    scanf("%c",&fh);

    switch(fh)
    {
    case '+':
        c=a+b;
        break;
    case '-':
        c=a-b;
        break;
    case '*':
        c=a*b;
        break;
    case '/':
        c=a/b;
        break;
    }
    printf("%d",c);

    return jsq1;
}
int Time()//äº”ç§’åˆ·æ–°ä¸€æ¬¡æ—¶é—´
{   //int a=30;
    while(1)
    {   //a++;
        time_t t = time(NULL);
        struct tm tm = *localtime(&t);
        printf("\033[1;35mç°åœ¨æ—¶é—´æ˜¯ï¼š%d-%02d-%02d %02d:%02d:%02d\n",
               tm.tm_year + 1900, tm.tm_mon + 1,tm.tm_mday,
               tm.tm_hour, tm.tm_min, tm.tm_sec);
        sleep(5);//å»¶æ—¶5ç§’
    }
}



