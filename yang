    {
        printf("çœŸ\n");
    }
    else
    {
        printf("å‡\n");
    }



    printf("æˆ–é—¨,è¾“å…¥ä¸¤ä¸ªæ•°å­—\n");//æˆ–é—¨
    scanf("%d%d",&a,&b);
    if (a>0 || b>0)
    {
        printf("çœŸ\n");
    }
    else
    {
        printf("å‡\n");
    }



    bool  kgk ;
    printf ("éé—¨,è¾“å…¥ä¸€ä¸ªæ•°å­—(0æˆ–1)\n");
    scanf ("%d",&kgk);

    kgk=!(kgk);


