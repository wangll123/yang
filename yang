void caiSZ ()
{
    int b;
    int c=1;
    srand((unsigned int)time(NULL));
    //åˆ·æ–°æ—¶é—´æˆ³;
    int a=rand()%100+1;
    printf("\033[1;32mçŒœä¸€ä¸ª1-100çš„æ•°å­—\n");
    // printf("%d\n",a);
    while(1)
    {
        scanf("%d",&b);
        if(b<a)
        {
            printf("çŒœå°äº†ï¼Œå®ğŸ’˜ğŸ’˜\n");
        }
        else if(b>a)
        {
            printf("çŒœå¤§äº†ï¼Œå®ğŸ¤”ğŸ¤”\n");
        }
        else
        {
            printf("çŒœå¯¹äº†ï¼Œå®è´ğŸ˜˜ğŸ˜˜\n");
            break;
        }
        c++;
    }
    printf("ä½ ä¸€å…±çŒœäº† %d æ¬¡\n",c);

}
void cfb()
{

    int i, j;
    for (i = 1; i <= 9; i++)
    {
        for (j = 1; j <= i; j++)
        {
            printf("%d*%d=%-2d ", j, i, i*j);
        }
        printf("\n");
    }

}

