int ysf()  //è¿ç®—ç¬¦
{   int  a,b,c;
    int A,B,C,D,E,F,G,H,I,J,K,L,M;
    A=5;   //é™¤æ³•è¿ç®—
    B=2;
    float NB = (float)A / (float)B;
    printf("%f\n",NB);


    // é™¤æ³•å–ä½™æ•°(å–æ¨¡)
    C = A % B;//5Ã·2 ä½™1
    printf("%d\n",C);


    //è‡ªåŠ 1 D++   ++D
    //è‡ªå‡åŒç†
    D=1;     //D++  å…ˆè¿”å›å€¼ï¼Œåœ¨åŠ åŠ 
    printf("%d\n", D++);
    printf("%d\n", D);
    D=1;     //++D  å…ˆåŠ åŠ ï¼Œå†è¿”å›å€¼
    printf("%d\n",++D);
    printf("%d\n",++D);

    //å¹³æ–¹è¿ç®— pow(åº•æ•°ï¼Œå¹‚)
    E=5;
    F=2;
    J = pow (E,F);
    printf("%d\n",J);

    /*å¼€æ ¹å·è¿ç®—
    å¦‚æœå‚æ•°æ˜¯ double ç±»å‹ï¼Œä½¿ç”¨ sqrt
    å¦‚æœå‚æ•°æ˜¯ float ç±»å‹ï¼Œä½¿ç”¨ sqrtf
    å¦‚æœå‚æ•°æ˜¯ long double ç±»å‹ï¼Œä½¿ç”¨ sqztl*/
    K = 36;
    M = sqrt (K);
    printf("%d\n",M);

    /* èƒ½å¤Ÿæ”¾åœ¨ç­‰å·å·¦è¾¹ï¼Œè¢«èµ‹å€¼å«åšå·¦å€¼
    åªèƒ½æ”¾åœ¨ç­‰å·å³è¾¹ï¼Œç»™åˆ«äººèµ‹å€¼ï¼Œå«åšå³å€¼
    å¸¸é‡(å­—é¢é‡)åªèƒ½åšå³å€¼
    å˜é‡æ˜¯å·¦å€¼ï¼Œä¹Ÿå¯ä»¥å½“åšå³å€¼*/


    a += 3;//a =a+3;
    a -= 3;//a =a-3;
    a *= 3;//a =a*3;
    a /= 3;//a =a/3;
    a %= 3;//a =a%3;



    a!=b;//aâ‰ b

    return ysf;


}

int shu()
{
    /*è¾“å‡ºæ¯ä¸ªæ•°æ®ç±»å‹åœ¨å†…å­˜ä¸­å ç”¨çš„å­—èŠ‚*/
    int nnnn =sizeof(char);
    printf("\033[1;33m%d\n",nnnn);
    int n=     sizeof(short);
    printf("%d\n",n);
    int nn =sizeof(int);
    printf("%d\n",nn);
    int nnn =sizeof(long);
    printf("%d\n\033[1;0m",nnn);
    return shu;
}


int jsq ()
{
    int a,b,dan ;
    char fh;

    printf("è¯·è¾“å…¥å·¦å€¼\n");
    scanf("%d",&a);
    printf("è¯·è¾“å…¥å³å€¼\n");
    scanf("%d",&b);


    /* char lj[1024];//æ¸…ç©ºç¼“å­˜å™¨ç©ºé—´
     fgets(lj,1024,stdin);*/
    fflush(stdin);

    printf("è¯·è¾“å…¥è¿ç®—ç¬¦\n");
    scanf("%c",&fh);

    if (fh =='+')
    {
        dan = a + b;

    }
    else if(fh=='-')
    {
        dan = a - b;

    }
    else if(fh=='*')
    {
        dan = a * b;

    }
    else if(fh=='/')
    {
        dan = a / b;

    }
    else
    {
        printf("ä¸æ”¯æŒ");
    }
    printf("%d",dan );


    return jsq;


}

void XQ (black, yellow, red)//6ï¼Œ5ï¼Œ3ã€‚
{
    /*ä»¥ä¸‹å‡½æ•°funçš„åŠŸèƒ½æ˜¯ä»3ä¸ªçº¢çƒ(x)ã€
    5ä¸ªç™½çƒ(y)ã€6ä¸ªé»‘çƒ(z)
    ä¸­ä»»æ„å–å‡º8ä¸ª,ä¸”å…¶ä¸­å¿…é¡»è¦æœ‰çº¢çƒå’Œç™½çƒã€‚
    è¯·ç¼–å†™å‡½æ•°ï¼Œè¾“å‡ºæ‰€æœ‰æ–¹æ¡ˆã€‚*/
    for(int i=1; i<=red; i++)
    {   for(int j=1; j<=yellow; j++)
        {
            num++;
            H=8-i-j;
            printf("\033[1;32mæ–¹æ¡ˆ%d çº¢:%d é»„:%d é»‘:%d\n\033[1;0m",num,i,j,H);
        }
    }


    // return XQ;
}
void sjs ()
{   //æ— é™è·å–éšæœºæ•°

    while(1)
    {
        srand((unsigned int)time(NULL));
        //åˆ·æ–°æ—¶é—´æˆ³;
        int a=rand()%2+0;
        //éšæœºæ•°0------1
        printf("%d\n",a);
        sleep(1);//å»¶æ—¶1ç§’
    }

}


